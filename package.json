{
	"name": "planetscale-auth-test",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"db:generate": "prisma generate --schema=./db/schema.prisma",
		"db:reset": "prisma migrate reset --schema=./db/schema.prisma --skip-seed",
		"db:push": "prisma db push --schema=./db/schema.prisma",
		"db:seed": "prisma db seed",
		"db:studio": "prisma studio --schema=./db/schema.prisma",
		"now-build": "yarn db:generate && next build"
	},
	"dependencies": {
		"@next-auth/prisma-adapter": "^1.0.4",
		"@prisma/client": "4.4.0",
		"bcryptjs": "^2.4.3",
		"next": "12.3.1",
		"next-auth": "^4.12.2",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.3",
		"@types/bcryptjs": "^2.4.2",
		"@types/node": "^18.7.23",
		"@types/react": "18.0.21",
		"@types/react-dom": "18.0.6",
		"autoprefixer": "^10.4.12",
		"eslint": "8.24.0",
		"eslint-config-next": "12.3.1",
		"postcss": "^8.4.16",
		"prisma": "^4.4.0",
		"tailwindcss": "^3.1.8",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4"
	},
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	}
}
